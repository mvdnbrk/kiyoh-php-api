{
    "name": "mvdnbrk/kiyoh-php-api",
    "description": "KiyOh API client for PHP",
    "keywords": [
        "php",
        "api",
        "kiyoh",
        "reviews"
    ],
    "homepage": "https://github.com/mvdnbrk/kiyoh-php-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "homepage": "https://github.com/mvdnbrk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "composer/ca-bundle": "^1.1",
        "guzzlehttp/guzzle": "^6.3",
        "tightenco/collect": "^5.7"
    },
    "require-dev": {
        "larapack/dd": "^1.1",
        "nunomaduro/collision": "^2.1",
        "phpunit/phpunit": "^7.0",
        "squizlabs/php_codesniffer": "^3.4",
        "vlucas/phpdotenv": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\Kiyoh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\Kiyoh\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvdnbrk\\Kiyoh\\KiyohServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "check-style": "phpcs -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src",
        "fix-style": "phpcbf -p --standard=PSR2 --runtime-set ignore_errors_on_exit 1 --runtime-set ignore_warnings_on_exit 1 src"
    },
    "config": {
        "sort-packages": true
    }
}
