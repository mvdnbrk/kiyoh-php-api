{
    "name": "mvdnbrk/kiyoh-php-api",
    "description": "KiyOh API client for PHP",
    "keywords": [
        "kiyoh",
        "api",
        "reviews",
        "laravel"
    ],
    "homepage": "https://github.com/mvdnbrk/kiyoh-php-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.3 || ^8.0",
        "composer/ca-bundle": "^1.2",
        "guzzlehttp/guzzle": "^7.1",
        "illuminate/collections": "^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.3.3",
        "orchestra/testbench": "^6.0",
        "phpunit/phpunit": "^9.4",
        "symfony/var-dumper": "^5.0",
        "vlucas/phpdotenv": "^5.2"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvdnbrk\\Kiyoh\\KiyohServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\Kiyoh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\Kiyoh\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
