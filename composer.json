{
    "name": "mvdnbrk/kiyoh-php-api",
    "description": "KiyOh API client for PHP",
    "keywords": [
        "kiyoh",
        "api",
        "reviews",
        "laravel"
    ],
    "homepage": "https://github.com/mvdnbrk/kiyoh-php-api",
    "license": "MIT",
    "authors": [
        {
            "name": "Mark van den Broek",
            "email": "mvdnbrk@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.2 || ^8.0",
        "composer/ca-bundle": "^1.2",
        "guzzlehttp/guzzle": "^6.5 || ^7.1",
        "tightenco/collect": "^5.8 || ^6.0 || ^7.0 || ^8.0"
    },
    "require-dev": {
        "orchestra/testbench": "^3.8 || ^4.5 || ^5.0",
        "phpunit/phpunit": "^8.5 || ^9.0",
        "symfony/var-dumper": "^4.3 || ^5.0",
        "vlucas/phpdotenv": "^4.1"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Mvdnbrk\\Kiyoh\\KiyohServiceProvider"
            ]
        }
    },
    "autoload": {
        "psr-4": {
            "Mvdnbrk\\Kiyoh\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Mvdnbrk\\Kiyoh\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
